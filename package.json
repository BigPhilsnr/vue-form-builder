    "babel-eslint": "^10.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "node-forge": ">=0.10.0"
